{
    "name": "Twilight Dark",

    "colors": {
    },

    "tokenColors": [
        {
            "settings": {
                "foreground": "#a7a7a7",
                "background": "#1e1e1e",

                "lineHighlight": "#282828",
                "selection": "#464b50",
                "inactiveSelection": "#323537",

                "findMatchHighlight": "#323537",
                "currentFindMatchHighlight": "#464b50",

                "referenceHighlight": "#0000ff",
                "activeLinkForeground": "#ff0000",


                "!wordHighlight": "#323537",
                "!wordHighlightStrong": "#323537",
                "!hoverHighlight": "#323537",

                "!rangeHighlight": "#00ff00",
                "!selectionHighlight": "#323537"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },

        {
            "scope": "comment",
            "settings": {
                "foreground": "#5f5a60"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#cda869"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#afc4db"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#cf6a4c"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#cda869"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",

                "source.css.less entity.other.attribute-name.id",

                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
